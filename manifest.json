{
  "name": "pass-chrome",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome extension frontend for `pass` -- see http://passwordstore.org",
  "homepage_url": "https://github.com/liminalsoftware/pass-chrome#readme",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "background/background.html",
    "persistent": false
  },
  "options_page": "options/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Password Store",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "fileBrowserHandler",
    "history",
    "notifications",
    "tabs",
    "geolocation"
  ],
  "omnibox": {
    "keyword": "pass"
  }
}